{
  "name": "@bitbeat/redis",
  "version": "0.0.1",
  "description": "The redis module for bitbeat.",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "keywords": [
    "bitbeat",
    "framework",
    "modular",
    "backend",
    "minimal",
    "redis",
    "cache"
  ],
  "scripts": {
    "start": "npx @bitbeat/core start",
    "build": "npx gulp default",
    "watch": "npx gulp watch",
    "analyse:dependencies": "npx bundle-phobia -p package.json",
    "analyse:licenses": "yarn licenses list",
    "lint": "eslint .",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitbeatjs/redis.git"
  },
  "author": "Oliver Freudrich <info@oliverfreudrich.com> (https://oliverfreudrich.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitbeatjs/redis/issues"
  },
  "homepage": "https://github.com/bitbeatjs/redis#readme",
  "dependencies": {
    "ioredis": "^4.17.3"
  },
  "peerDependencies": {
    "@bitbeat/core": "^0.0.14"
  },
  "devDependencies": {
    "@bitbeat/core": "^0.0.14",
    "@types/debug": "^4.1.5",
    "@types/ioredis": "^4.17.4",
    "@types/node": "^14.10.1",
    "@types/node-cron": "^2.0.3",
    "@types/pino": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "gulp": "^4.0.2",
    "gulp-changed-in-place": "^2.3.0",
    "gulp-eslint": "^6.0.0",
    "gulp-terser": "^1.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "merge-stream": "^2.0.0",
    "prettier": "^2.1.1",
    "type-fest": "^0.16.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "prettier --write"
    ],
    "*.{js,css,md}": [
      "prettier --write"
    ]
  }
}
